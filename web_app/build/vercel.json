{
"_": "This is your Vercel config file. For more information see the global configuration documentation: https://vercel.com/docs/configuration#global",
  "collectMetrics": true,
  "redirects": [
      { "source": "/callback(?<id>[^/]+)", "destination": "/api/callback?id=$id", "permanent" : true}
    ],
    "routes": [
        { "src": "/callback/(?<id>[^/]+)", "dest": "/api/callback?id=$id" }
    ],
  "headers": [
    {
      "source": "https://auth.unstoppabledomains.com",
      "headers" : [
        {
          "key" : "Access-Control-Allow-Origin",
          "value" : "true"
        },
        {
          "key" : "s-maxage",
          "value" : 0
        }
      ]
    },
    {
      "source": "*",
      "headers" : [
        {
          "key" : "Access-Control-Allow-Origin",
          "value" : "true"
        }
      ]
    }
  ]

}